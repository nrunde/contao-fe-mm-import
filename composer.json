{
  "name": "ulpr/contao-fe-mm-import",
  "description": "Import data from file uploaded by frontend form file field.",
  "keywords": [
    "contao",
    "import",
    "csv",
    "frontend",
    "form"
  ],
  "type": "contao-bundle",
  "license": "LGPL-3.0-or-later",
  "authors": [
    {
      "name": "Nikolas Runde",
      "email": "nikolas.runde@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^5.5 || ^7.0",
    "contao/core-bundle": "^4.4",
    "contao-community-alliance/composer-plugin": "^3.0",
    "contao-community-alliance/url-builder": "^1.2",
    "symfony/http-foundation": "^3.3"
  },
  "autoload": {
    "psr-4": {
      "Ulpr\\ContaoFeMmImportBundle\\": "src/"
    },
    "classmap": [
            "src/Resources/contao/"
        ],
        "exclude-from-classmap": [
            "src/Resources/contao/config/",
            "src/Resources/contao/dca/",
            "src/Resources/contao/languages/",
            "src/Resources/contao/templates/"
        ]
  },
  "extra": {
    "contao-manager-plugin": "Ulpr\\ContaoFeMmImportBundle\\ContaoManager\\Plugin"
  }
}
